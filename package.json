{
  "name": "ts-node-setup",
  "version": "0.0.0",
  "description": "TS Node Setup Boilerplate",
  "main": "index.ts",
  "scripts": {
    "test": "npx jest --passWithNoTests",
    "test:watch": "npx jest --watch",
    "test:staged": "npx jest --findRelatedTests",
    "test:push": "npm run test -- --coverage",
    "build": "npx rimraf dist && tsc",
    "start": "node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "husky:prepare": "husky install"
  },
  "keywords": [],
  "author": "Fabricio Navarrete",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cross-env": "7.0.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
